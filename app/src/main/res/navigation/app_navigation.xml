<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/app_navigation"
    app:startDestination="@id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="id.daydream.harmonia.presentation.home.HomeFragment"
        android:label="@string/menu_title_home"
        tools:layout="@layout/fragment_home" />

    <activity
        android:id="@+id/navigation_analyze"
        android:name="id.daydream.harmonia.presentation.analyze.AnalyzeActivity"
        android:label="@string/menu_title_analyze"
        tools:layout="@layout/activity_analyze" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="id.daydream.harmonia.presentation.profile.ProfileFragment"
        android:label="@string/menu_profile_title"
        tools:layout="@layout/fragment_profile" />

